{
    "name": "tuna-vscode",
    "displayName": "Tuna-VSCODE",
    "description": "VSCode extension for the tuna haxe library!",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.109.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/JonkIsKindaCool/Tuna/tree/main/vscode"
    },
    "activationEvents": [
        "workspaceContains:**/config.tuna"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "tuna-vscode.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.109.0",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "typescript-eslint": "^8.54.0",
        "eslint": "^9.39.2",
        "typescript": "^5.9.3",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2"
    }
}